{
  "mcpServers": {
    "optibest-agentic-mcp": {
      "command": "node",
      "args": ["server.js"],
      "env": {
        "NODE_ENV": "production",
        "LOG_LEVEL": "info"
      },
      "disabled": false
    }
  },
  "coordination": {
    "federated_servers": [
      "optibest-vexl-mcp",
      "hrm-vexl-collab",
      "hrm-collaboration",
      "sequentialthinking",
      "cipher",
      "gemini",
      "optibest-agentic-mcp"
    ],
    "default_strategy": "hierarchical",
    "resource_limits": {
      "max_concurrent_agents": 10,
      "memory_limit_mb": 1024,
      "cpu_limit_percent": 80
    }
  },
  "optibest": {
    "default_rigor": "MACRO",
    "verification_methods": [1, 2, 3, 4, 5],
    "convergence_threshold": 0.001,
    "max_iterations": 15
  }
}
